<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>JavaServletsandJSP</display-name>
  <servlet>
  	<servlet-name>servletName</servlet-name>
  	<servlet-class>com.theo.add.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servletName</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>servletName2</servlet-name>
  	<servlet-class>com.theo.add.SquareServlet</servlet-class>
  </servlet> 
  <servlet-mapping>
  	<servlet-name>servletName2</servlet-name>
  	<url-pattern>/sq</url-pattern>
  </servlet-mapping>
  
  <context-param>
  	<param-name>name</param-name>
  	<param-value>Theo</param-value>
  </context-param>
  <context-param>
  	<param-name>phone</param-name>
  	<param-value>Samsung S8</param-value>
  </context-param>
</web-app>